basla=int(input("başlangıç değeri: "))
bitis=int(input("bitiş değeri: "))

while basla<=bitis:
    print(basla)
    basla+=1

i=100

while i>=1:
    print(i)
    i-=1


i=0
list=[]

while (i<5):
    sayi=int(input("sayı: "))
    list.append(sayi)
    i+=1

list.sort()
print(list)



username=""

while not username:
    username=input("kullanıcı adı:")

print("girilen username"+username)
